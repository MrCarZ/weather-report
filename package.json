{
  "name": "weather-report",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint": "eslint --cache --cache-location \".eslintcache/\" --ignore-path .eslintignore --ext .ts,.tsx .",
    "lint:fix": "eslint --cache --ignore-path .eslintignore --ext .ts,.tsx --fix .",
    "format": "prettier --ignore-path .gitignore --check \"src/**/*.+(js|jsx|ts|tsx)\"",
    "format:write": "prettier --ignore-path .gitignore --write \"src/**/*.+(js|jsx|ts|tsx|json)\""
  },
  "dependencies": {
    "antd": "^5.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "vite-plugin-svgr": "^3.2.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.3",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
